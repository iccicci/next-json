  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "name": "next-json",
  "prettier": {
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "jsxBracketSameLine": true,
    "printWidth": 200,
    "trailingComma": "none",
    "useTabs": false
