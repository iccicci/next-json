    "@types/node": "24.3.0",
    "eslint": "9.34.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sort-keys": "2.3.5",
    "express": "5.1.0",
    "jest": "29.7.0",
    "jest-environment-node-single-context": "29.4.0",
    "peggy": "5.0.6",
    "prettier": "3.6.2",
    "ts-jest": "29.4.1",
    "ts-pegjs": "4.2.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.41.0"
  },
  "engines": {
    "node": ">=14.0"
