  },
  "funding": {
    "url": "https://github.com/iccicci/next-json#donating"
  },
  "homepage": "https://github.com/iccicci/next-json#readme",
  "keywords": [
    "bigint",
    "buffer",
    "circular",
    "circular-reference",
    "custom-json",
    "date",
