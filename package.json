    "eval",
    "extended-json",
    "infinity",
    "javascript",
    "json",
    "map",
    "nan",
    "parse",
    "regexp",
    "repeated",
    "repeated-reference",
    "serialize",
    "set",
    "stringify",
    "undefined"
